public class FollowInfo {

    private String custName;     //客户名称 
    private String custPhoneNum; //客户电话
    private String action;
    private String actionStatus; //活动状态
    private String actionContent; //活动内容
    private String actionType;   //活动类型
    private long   doTime;       //执行时间
    private String doStatus;     //执行状态

    public String getCustName() {
        return custName;
    }

    public void setCustName(String n) {
        custName = n;
    }

    public String getCustPhoneNum() {
        return custPhoneNum;
    }

    public void setCustPhoneNum(String p) {
        custPhoneNum = p;
    }

    public String getAction() {
        return action;
    }

    public void setAction(String a) {
        action = a;
    }

    public String getActionStatus() {
        return actionStatus;
    }

    public void setActionStatus(String a) {
        actionStatus = a;
    }

    public String getActionContent() {
        return actionContent;
    }

    public void setActionContent(String actionContent) {
        this.actionContent = actionContent;
    }

    public String getActionType() {
        return actionType;
    }

    public void setActionType(String actionType) {
        this.actionType = actionType;
    }

    public long getDoTime() {
        return doTime;
    }

    public void setDoTime(long doTime) {
        this.doTime = doTime;
    }

    public String getDoStatus() {
        return doStatus;
    }

    public void setDoStatus(String doStatus) {
        this.doStatus = doStatus;
    }

}